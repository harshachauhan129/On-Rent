<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="au.edu.jcu.flatOnRent.onrent">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="@string/app_name"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme1."
        tools:replace="android:appComponentFactory">
        <activity android:name=".Seller.DetailsMain"></activity>
        <activity
            android:name=".Seller.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".Seller.Version" />
        <activity android:name=".upload_data.UploadForms" />

        <service android:name=".helper.GPS_Service" />

        <activity android:name=".Seller.SellerProfile" />
        <activity
            android:name=".FirebaseHelper.EnternNumber"
            android:theme="@style/AppTheme1." />
        <activity
            android:name=".Seller.TabbedViewUser"
            android:theme="@style/AppTheme1." />
        <activity android:name=".startingPages.LocationActivity" />
        <activity android:name=".startingPages.RegisterActivity" />
        <activity
            android:name=".startingPages.SplashScreen"
            android:theme="@style/AppTheme1.">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>